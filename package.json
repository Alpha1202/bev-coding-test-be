{
  "name": "inbev-coding-test",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build-server": "babel src --out-dir src",
    "build": "npm run build-server",
    "clean": "rm -rf dist && mkdir dist",
    "dev": "cross-env NODE_ENV=development npm run migrate && nodemon  --exec babel-node --max-old-space-size=8192 ./src/app.js ",
    "local": "cross-env NODE_ENV=local npm run migrate && nodemon  --exec babel-node --max-old-space-size=8192 ./src/app.js ",
    "start": "npm run build && npm run migrate && node src/app.js",
    "migrate": "sequelize db:migrate",
    "undo:migrate": "sequelize db:migrate:undo:all",
    "seed": "sequelize db:seed:all",
    "pretest": "npm run undo:migrate && npm run migrate && npm run seed",
    "dummy:seed": "node ./src/db/test-seeder.js --exec babel-node"
  },
  "dependencies": {
    "@babel/cli": "^7.17.6",
    "@babel/core": "^7.17.9",
    "@babel/node": "^7.16.8",
    "@babel/plugin-transform-modules-commonjs": "^7.17.9",
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "@babel/register": "^7.17.7",
    "@babel/runtime": "^7.17.9",
    "@sendgrid/mail": "^7.6.2",
    "babel-plugin-istanbul": "^6.1.1",
    "babel-plugin-module-resolver": "^4.1.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.0",
    "cloudinary": "^1.29.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "express-validator": "^6.14.0",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.4",
    "multer-storage-cloudinary": "^4.0.0",
    "mysql2": "^2.3.3",
    "regenerator-runtime": "^0.13.9",
    "sequelize": "^6.18.0",
    "sequelize-cli": "^6.4.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "eslint": "^8.12.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.26.0",
    "nodemon": "^2.0.15"
  }
}
